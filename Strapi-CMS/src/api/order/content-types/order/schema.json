{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": true
    },
    "orderid": {
      "type": "uid",
      "required": true
    },
    "products": {
      "type": "json",
      "required": true
    },
    "paymentinfo": {
      "type": "json"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "text"
    },
    "transactionid": {
      "type": "string"
    },
    "bill": {
      "type": "decimal"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Payment Success & Delivered",
        "Payment Success & Not Delivered",
        "Payment Failed"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    },
    "tid": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::transaction.transaction",
      "mappedBy": "order"
    }
  }
}
